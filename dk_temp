$RegistryBase = 'HKLM:\SOFTWARE'
$RegistryKey = 'FortifiedData'
$BasePath = "$((Get-Item -Path "$RegistryBase\$RegistryKey").GetValue("ClientSideBasePath"))\ClientManagement"
$BasePath
$Config = (Get-Content "$BasePath\Config.json" | Out-String | ConvertFrom-Json)
Set-AWSCredential -AccessKey $Config.CloudStorage.AccessKey -SecretKey $Config.CloudStorage.SecretKey
$Bucket = $Config.CloudStorage.Bucket
$SubPrefix = $Config.CloudStorage.SubPrefix
$ResetPrefix = "reset/$SubPrefix"
"Trying to connect to S3 bucket"
Get-S3object -BucketName $Bucket -KeyPrefix "$ResetPrefix/"
